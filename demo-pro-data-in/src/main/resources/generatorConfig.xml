<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>
    <!-- <properties resource="application.properties"></properties> -->
    <!-- Database Driver -->
    <classPathEntry
            location="C:\Users\baizy\AppData\Roaming\JetBrains\IntelliJIdea2022.2\jdbc-drivers\Xerial SQLiteJDBC\3.38.0\sqlite-jdbc-3.38.0.jar"/>

    <context id="demo-pro-001-sqlite">
        <!-- optional，旨在创建class时，对注释进行控制 -->
        <commentGenerator>
            <property name="suppressAllComments" value="true"></property>
        </commentGenerator>

        <jdbcConnection driverClass="org.sqlite.JDBC"
                        connectionURL="jdbc:sqlite:H:\Database\Sqlite\demo-pro-001">
        </jdbcConnection>

        <javaModelGenerator targetPackage="com.example.demoprodata.in.dao.entity"
                            targetProject="src/main/java">
            <property name="trimStrings" value="true"/>
        </javaModelGenerator>

        <sqlMapGenerator targetPackage="mapper"
                         targetProject="src/main/resources">
        </sqlMapGenerator>

        <javaClientGenerator type="XMLMAPPER"
                             targetPackage="com.example.demoprodata.in.dao.mapper"
                             targetProject="src/main/java">
        </javaClientGenerator>

        <table tableName="demo_pro_001"
               domainObjectName="DemoPro001"
               enableCountByExample="true"
               enableUpdateByExample="true"
               enableDeleteByExample="true"
               enableSelectByExample="true"
               selectByExampleQueryId="true">
        </table>
    </context>

</generatorConfiguration>